{
    "name": "Twitter Filter",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Hiding the World Away",
    "homepage_url": "https://l1tt.cc",
    "content_scripts": [{
        "matches": ["https://twitter.com/*", "http://twitter.com/*"],
        "js": ["inject.js"]
    }],
    "browser_action": {
        "default_title": "Twitter Filter"
    },
    "permissions": [
        "https://twitter.com/*",
        "http://twitter.com/*",
        "tabs",
        "activeTab"
    ]
}